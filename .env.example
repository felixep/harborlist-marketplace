# HarborList Local Development Environment Variables
# Copy this file to .env.local and customize as needed

# Frontend Configuration
VITE_API_URL=http://local-api.harborlist.com:3001/api
VITE_ENVIRONMENT=local

# Backend Configuration
NODE_ENV=development

# Database Configuration
DYNAMODB_ENDPOINT=http://localhost:8000
DYNAMODB_REGION=us-east-1
USERS_TABLE=boat-users
LISTINGS_TABLE=harborlist-listings
SESSIONS_TABLE=boat-sessions
ANALYTICS_TABLE=boat-analytics
AUDIT_LOGS_TABLE=boat-audit-logs

# AWS Services (Local)
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_REGION=us-east-1
S3_ENDPOINT=http://localhost:4566
SES_ENDPOINT=http://localhost:4566
S3_BUCKET=harborlist-uploads-local

# Authentication
JWT_SECRET=local-dev-secret-harborlist-2025
JWT_EXPIRES_IN=7d

# CORS Configuration
CORS_ORIGINS=http://local.harborlist.com:3000,http://localhost:3000

# API Configuration
API_BASE_URL=http://local-api.harborlist.com:3001
FRONTEND_URL=http://local.harborlist.com:3000

# Email Configuration (Local SES)
FROM_EMAIL=noreply@harborlist.local
ADMIN_EMAIL=admin@harborlist.local

# File Upload Configuration
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=jpg,jpeg,png,webp

# Logging
LOG_LEVEL=debug

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379